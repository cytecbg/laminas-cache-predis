{
    "name": "cytec/laminas-cache-predis",
    "description": "Redis adapter for Laminas Framework 3 with tagging support",
    "license": "BSD-3-Clause",
    "keywords": [
        "zf2",
        "zf3",
        "cache",
        "redis",
        "predis",
        "tag",
        "taggable",
        "laminas"
    ],
    "homepage": "https://github.com/cytecbg/laminas-cache-predis",
    "autoload": {
        "psr-4": {
            "Cytec\\Cache\\": "src/Cache/"
        }
    },
    "require": {
        "php": ">=7.4",
        "laminas/laminas-cache": "^2.11",
        "predis/predis": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "laminas/laminas-serializer": "^2.10"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "test": "php vendor/bin/phpunit"
    }
}
